{
  "name": "aqeelraza-dev",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "build": "nest build",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:integration": "jest --config ./test/jest-integration.json",
    "test:integration:watch": "jest --config ./test/jest-integration.json --watch",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:load": "k6 run test/load/otp-auth-load.js",
    "test:load:rate-limit": "k6 run test/load/rate-limit-stress.js",
    "test:load:concurrent": "k6 run test/load/concurrent-users.js",
    "test:all": "npm run test && npm run test:integration && npm run test:e2e",
    "docker:test:up": "docker-compose -f docker-compose.test.yml up -d",
    "docker:test:down": "docker-compose -f docker-compose.test.yml down",
    "docker:test:logs": "docker-compose -f docker-compose.test.yml logs -f",
    "cdk": "cdk",
    "cdk:deploy": "cdk deploy --all",
    "cdk:destroy": "cdk destroy --all",
    "smoke:persistence": "ts-node scripts/smoke-persistence.ts"
  },
  "dependencies": {
    "@aws-sdk/client-cognito-identity-provider": "^3.713.0",
    "@aws-sdk/client-dynamodb": "^3.713.0",
    "@aws-sdk/client-kms": "^3.713.0",
    "@aws-sdk/client-secrets-manager": "^3.713.0",
    "@aws-sdk/client-ses": "^3.713.0",
    "@aws-sdk/client-sns": "^3.713.0",
    "@aws-sdk/lib-dynamodb": "^3.713.0",
    "@nestjs/common": "^10.4.15",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "^10.4.15",
    "@nestjs/platform-express": "^10.4.15",
    "@nestjs/throttler": "^6.2.1",
    "aws-cdk-lib": "^2.174.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "constructs": "^10.4.2",
    "jsonwebtoken": "^9.0.2",
    "libphonenumber-js": "^1.11.16",
    "nanoid": "^5.0.9",
    "nestjs-pino": "^4.1.0",
    "pino": "^9.6.0",
    "pino-http": "^10.3.0",
    "pino-pretty": "^13.0.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "ulid": "^2.3.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.4.8",
    "@nestjs/schematics": "^10.1.4",
    "@nestjs/testing": "^10.4.15",
    "@playwright/test": "^1.56.1",
    "@types/aws-lambda": "^8.10.157",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/k6": "^1.4.0",
    "@types/node": "^22.10.2",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^8.18.1",
    "@typescript-eslint/parser": "^8.18.1",
    "aws-cdk": "^2.174.0",
    "eslint": "^9.17.0",
    "jest": "^29.7.0",
    "k6": "^0.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "testcontainers": "^11.8.1",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2"
  }
}
